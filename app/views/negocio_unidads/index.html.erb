<h1>Listing negocio_unidads</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @negocio_unidads.each do |negocio_unidad| %>
      <tr>
        <td><%= negocio_unidad.nombre %></td>
        <td><%= link_to 'Show', negocio_unidad %></td>
        <td><%= link_to 'Edit', edit_negocio_unidad_path(negocio_unidad) %></td>
        <td><%= link_to 'Destroy', negocio_unidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Negocio unidad', new_negocio_unidad_path %>
